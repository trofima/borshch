{
  "name": "@borshch/utilities",
  "description": "Borshch utilities",
  "version": "2.1.2",
  "author": "trofima",
  "license": "ISC",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "import": "./dist/borshch-utilities.js",
    "require": "./dist/borshch-utilities.cjs"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "spy",
    "function spy",
    "deferred",
    "atom",
    "mixin",
    "event emitter"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trofima/borshch.git"
  },
  "homepage": "https://github.com/trofima/borshch/packages/utilities#readme",
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "rm -rf dist/*",
    "build": "rollup -c",
    "test": "mocha -u tdd --timeout 10000 --colors './src/**/*.test.js'",
    "lint": "eslint --fix",
    "lint-staged": "lint-staged"
  },
  "pre-commit": [
    "lint"
  ],
  "pre-push": [
    "test"
  ],
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "chai": "^5.1.2",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "lint-staged": "^15.2.11",
    "mocha": "^11.0.1",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.4",
    "rollup": "^4.29.1",
    "terser": "^5.37.0"
  },
  "dependencies": {
    "object-hash": "^3.0.0",
    "rambda": "^9.4.1"
  }
}
